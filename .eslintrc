{
  "extends": ["plugin:prettier/recommended"],
  "globals": {
    "Sortable": true,
    "Promise": true,
    "d3": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "prettier/prettier": "error"
  }
}
